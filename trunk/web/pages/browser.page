<div id="browseView" class="contentPart">
	<div id="browser">{browsercontainer}</div>
	<div id="noInformation">{noinfocontainer}</div>
	<div id="duplicatecontainer">{duplicatecontainer}</div>
</div>

{browser-start}
<h2>Durchsuchen</h2>

<ul class="keyList">
	{keyListItems}
</ul>

{fileListItems}
{browser-end}

{browserFileListItemSingle-start}
<table class="{class}">
	<tr>
		<th>{title}</th>
	</tr>
	{fileListItemSingles}
</table>
{browserFileListItemSingle-end}

{browserFileListItem-start}
	<tr>
		<td><a href="?action=showFileDetails&fileId={itemId}">{name}</a></td>
	</tr>
{browserFileListItem-end}

{browserKeyListItem-start}
	<li><a href="javascript:void(0)">{key}</a></li>
{browserKeyListItem-end}


{duplicate-start}
<h2>Doppelte Einträge</h2>
<p>Aktuell ist diese Anzeige nicht verfügbar.</p>
{duplicate-end}

{noinformation-start}
<h2>Keine Informationen</h2>
<p>Für folgende <span class="noInformationCounter">{noinformationcounter}</span> Einträge gibt es keine weiteren Informationen von den möglichen Collectoren:</p>
{noinformationTables}
{noinformation-end}

{noinformationTable-start}
<h3>{collectorName}</h3>
<table>
	<tr>
		<th>ID</th>
		<th>Titel</th>
		<th></th>
	</tr>
	{noinformationItems}
</table>
{noinformationTable-end}

{noinformationItem-start}
<tr class="{oddeven}">
	<td>{noInfoItemId}</td>
	<td>{noInfoItemTitle}</td>
	<td><a href="?action=showFileDetails&fileId={noInfoItemId}">Anzeigen</a></td>
</tr>
{noinformationItem-end}